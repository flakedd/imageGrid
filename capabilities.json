{
    "dataRoles": [
        {
            "displayName": "Estilo",
            "name": "item_parent",
            "kind": "Grouping"
        },
        {
            "displayName": "Imagen",
            "name": "url_image",
            "kind": "Measure"
        },
        {
            "displayName": "Margen",
            "name": "mg",
            "kind": "Measure"
        }
    ],
    "objects": {
        "dataPoint": {
            "properties": {
                "defaultColor": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "showAllDataPoints": {
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fillRule": {
                    "type": {
                        "fill": {}
                    }
                },
                 "fontSize": {
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "for": {
                        "in": "item_parent"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "url_image"
                            }
                        },
                        {
                            "bind": {
                                "to": "mg"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "privileges": [
        {
            "name": "WebAccess",
            "essential": true,
            "parameters": [ "https://www.cuidadoconelperro.com.mx/" ]
        },
        {
            "name": "ExportContent",
            "essential": false
        }
    ]
}
